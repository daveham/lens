{
  "private": true,
  "name": "@lens/app",
  "homepage": "/lensapp",
  "version": "0.1.0",
  "description": "inference lens generative image system - manager app",
  "main": "index.js",
  "engines": {
    "node": ">=8.11.0",
    "npm": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:daveham/lens.git"
  },
  "author": "daveham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daveham/lens/issues"
  },
  "dependencies": {
    "@lens/data-jobs": "^0.1.0",
    "@lens/image-descriptors": "^0.1.0",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.39",
    "bfj": "6.1.1",
    "camelcase": "^5.2.0",
    "classnames": "^2.2.6",
    "debug": "^4.1.1",
    "dotenv": "6.2.0",
    "fs-extra": "7.0.1",
    "font-awesome": "^4.7.0",
    "history": "^4.6.3",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-dev-utils": "10.1.0",
    "react-dom": "^16.12.0",
    "react-error-overlay": "^6.0.4",
    "react-fontawesome": "^1.7.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "socket.io-client": "^2.0.3",
    "uuid": "^3.3.3",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "eslintConfig": {
    "extends": "plugin:react-app/recommended",
    "rules": {
      "react-app/react/react-in-jsx-scope": ["warn"],
      "no-unused-vars": ["error", { "ignoreRestSiblings": true, "varsIgnorePattern": "^[iI]gnored" }]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest-enzyme": "^7.0.2",
    "prettier": "^1.18.2",
    "react-docgen-typescript": "^1.16.2",
    "react-styleguidist": "^9.1.11",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4"
  }
}
