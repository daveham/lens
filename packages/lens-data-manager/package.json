{
  "private": true,
  "name": "@lens/data-manager",
  "version": "0.1.0",
  "description": "inference lens generative image system - data manager",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:daveham/lens.git"
  },
  "author": "daveham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daveham/lens/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "testx": "better-npm-run test",
    "test": "jest",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "flow:check": "node bin/flow-check"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/register-compile"
    },
    "dev": {
      "command": "nodemon --debug=192.168.20.20:5859 bin/register-server"
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "jest"
    },
    "xtest": {
      "command": "node -r dotenv/config ./node_modules/karma/bin/karma start bin/karma.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "xdev": {
      "command": "nodemon -r dotenv/config bin/register",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "xdeploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "xstart": {
      "command": "node -r dotenv/config bin/register"
    }
  },
  "dependencies": {
    "@lens/image-descriptors": "^0.1.0",
    "@lens/data-jobs": "^0.1.0",
    "better-npm-run": "0.0.9",
    "connect-history-api-fallback": "^1.1.0",
    "cssnano": "^3.3.2",
    "fs-extra": "^0.26.3",
    "glob": "^6.0.4",
    "gm": "^1.21.1",
    "history": "^2.0.0",
    "isomorphic-fetch": "^2.2.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "react-test-renderer": "^15.6.1"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|xspec))\\.jsx?$",
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
