{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": true
        },
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "useBuiltIns": "entry"
      }
    ]
  ],
  "plugins": [
    "@babel/transform-runtime",
    "@babel/plugin-proposal-export-default-from",
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    ["transform-gl-matrix", {
      "glMatrixArray": false
    }]
  ]
}
